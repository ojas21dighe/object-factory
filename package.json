{
  "name": "object-factory",
  "version": "1.2.0",
  "description": "A browser extension for capturing the Page Object Models and be used in automation",
  "author": "Ojas Ulhas Dighe - odighe@saleforce.com",
  "homepage": "https://www.google.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint --ext .js,.vue src",
    "prettier": "prettier \"src/**/*.{js,vue}\"",
    "prettier:write": "npm run prettier -- --write",
    "clean": "rimraf build",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode=production",
    "build:dev": "npm run clean && cross-env NODE_ENV=development webpack --mode=development",
    "zip:chrome": "node scripts/build-chrome.js",
    "zip": "npm run zip:chrome",
    "watch": "npm run build -- --watch",
    "watch:dev": "npm run build:dev -- --watch",
    "launch:chrome": "cross-env LAUNCH_CHROME=true HMR=true npm run watch:dev",
    "version": "cd scripts && node release.js && cd ./",
    "release": "npm run version && npm run build && npm run zip:chrome",
    "pretty-quick": "pretty-quick --staged"
  },
  "dependencies": {
    "@mdi/js": "^7.0.96",
    "babel-plugin-rewire": "^1.2.0",
    "caniuse-lite": "^1.0.30001423",
    "file-saver": "^2.0.5",
    "fs": "^0.0.1-security",
    "lodash": "^4.17.20",
    "node-fs": "^0.1.7",
    "scroll-into-view-if-needed": "^2.2.26",
    "simmerjs": "^0.5.6",
    "vue": "2.6.14",
    "vuelidate": "^0.7.6",
    "vuetify": "2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.14.7",
    "@types/chrome": "^0.0.199",
    "archiver": "^5.2.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chrome-launch": "^1.1.4",
    "copy-webpack-plugin": "^8.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.13.0",
    "file-loader": "^6.2.0",
    "husky": "^6.0.0",
    "inquirer": "^8.1.2",
    "jest": "^26.6.3",
    "json-schema": ">=0.4.0",
    "mini-css-extract-plugin": "^1.6.2",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "sass": "^1.35.2",
    "sass-loader": "^12.6.0",
    "semver": "^7.3.4",
    "terser-webpack-plugin": "^5.1.4",
    "trim-newlines": ">=3.0.1",
    "vue-loader": "15.9.8",
    "vue-template-compiler": "2.6.14",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.3.1",
    "webpack-shell-plugin-next": "^2.1.1"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "net": false,
    "tls": false
  }
}
